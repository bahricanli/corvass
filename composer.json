{
    "name": "bahricanli/corvass",
    "description": "Sends SMS using Corvass API",
    "homepage": "https://github.com/bahricanli/corvass",
    "version": "1.1.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bahri Meriç Canlı",
            "email": "bahri@bahri.info",
            "homepage": "https://www.bahri.info",
            "role": "Developer"
        },
        {
            "name": "Hilmi Erdem KEREN",
            "email": "erdemkeren@gmail.com",
            "homepage": "http://www.erdemkeren.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7",
        "bahricanli/corvass-php": "^1.1",
        "illuminate/notifications": "8.*",
        "illuminate/queue": "8.*",
        "illuminate/support": "8.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Corvass\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Corvass\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:bahricanli/corvass-php.git"
        }
    ]
}
