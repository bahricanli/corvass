{
    "name": "bmericc/jet-sms",
    "description": "Sends SMS using JetSms API",
    "homepage": "https://github.com/bmericc/jet-sms",
    "license": "MIT",
    "authors": [
        {
            "name": "Hilmi Erdem KEREN",
            "email": "erdemkeren@gmail.com",
            "homepage": "http://www.erdemkeren.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "bmericc/jet-sms-php": "dev-stable",
        "illuminate/notifications": "^6.0|^7.0|^8.0",
        "illuminate/queue": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\JetSms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\JetSms\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:bmericc/jet-sms-php.git"
        }
    ]
}
