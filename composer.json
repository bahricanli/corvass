{
    "name": "bahricanli/corvass",
    "description": "Sends SMS using Corvass API",
    "homepage": "https://github.com/bahricanli/corvass",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bahri Meriç Canlı",
            "email": "bahri@bahri.info",
            "homepage": "https://www.bahri.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "bahricanli/corvass-php": "^1.0",
        "illuminate/notifications": "^5.3",
        "illuminate/queue": "^5.1|^5.2|^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Corvass\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Corvass\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:bahricanli/corvass-php.git"
        }
    ]
}
